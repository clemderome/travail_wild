class person {
    constructor(name, age) {
        this.name = name;
        this.age = age;
    }
    tellMyName(){
        console.log(`I am ${this.name}`);
    }
    tellMyAge(){
        console.log(`I am ${this.age} years old`)
    }
}

const john = new person('John', 40)
    john.tellMyName();
    john.tellMyAge();

const mary = new person('Mary', 35)
    mary.tellMyName();
    mary.tellMyAge();
    
